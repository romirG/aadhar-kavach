# Environment variables for Gender Tracker ML Backend
# Copy this file to .env and fill in your values

# API Keys for data.gov.in datasets
DATA_GOV_API_KEY=your_data_gov_api_key_here

# API Base URLs (placeholders - replace with actual endpoints)
API1_BASE=https://api.data.gov.in/resource
API2_BASE=https://api.data.gov.in/resource
API3_BASE=https://api.data.gov.in/resource

# Resource IDs for UIDAI datasets
ENROLMENT_RESOURCE_ID=ecd49b12-3084-4521-8f7e-ca8bf72069ba
DEMOGRAPHIC_RESOURCE_ID=19eac040-0b94-49fa-b239-4f2fd8677d53
BIOMETRIC_RESOURCE_ID=65454dab-1517-40a3-ac1d-47d4dfe6891c

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true

# ML Configuration
DEFAULT_RISK_THRESHOLD=0.85
RANDOM_SEED=42

# Security
API_TOKEN_SECRET=your_secret_token_here
ENABLE_PRIVACY_NOISE=false
NOISE_EPSILON=0.1

# Storage Paths
ARTIFACTS_DIR=./artifacts
MODELS_DIR=./models
